{   
    ?C geo:lat ?LAC.
    ?C geo:long ?LOC.
    ?SCOPE e:findall (
        ?T
        {   
            ?T geo:lat ?LA.
            ?T geo:long ?LO.
            ?T log:notEqualTo ?C.
            $?LA - ?LAC$ = ?DLA.
            $?LO - ?LOC$ = ?DLO.
            $math:exponentiation(?DLA * ?DLA + ?DLO * DLO, 0.5)$ math:lessThan 1.0.
        }
        ?L
    ).
} => {
    ?C t:answer ?L.
}.
