# N3X-0.2.0

@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix : <http://example.org/> .

:p1 :x "2" ; :y "3" .
:p2 :x "1" ; :y "5" .

{
  ?p1 :x ?x1 ; :y ?y1 .
  ?p2 :x ?x2 ; :y ?y2 .
  filter += ?p1 != ?p2 .
  ?dx   := ?x1 - ?x2 ;
    a :Difference .
  ?dy   := ?y1 - ?y2 .
  ?dist := math:sqrt(?dx * ?dx + ?dy * ?dy) .
} => {
  :result :value ?dist .
} .
